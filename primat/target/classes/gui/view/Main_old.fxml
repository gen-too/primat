<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<!-- style-sheet can also be set programatically -->

<AnchorPane prefHeight="707.0" prefWidth="806.0" stylesheets="@/gui/style/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbs.pprl.toolbox.client.gui.controller.MainController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="760.0" prefWidth="806.0" spacing="20.0">
         <children>
            <fx:include fx:id="menuBar" source="MenuBar.fxml" />
            <TabPane id="tab-pane-main" fx:id="tabPane" disable="true" prefHeight="443.0" prefWidth="786.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab fx:id="selectionTabPaneTab" text="Selection">
                    <content>
                      <fx:include fx:id="selectionTab" source="SelectionTab.fxml" />
                    </content>
                  </Tab>
                <Tab fx:id="dataCleaningTabPaneTab" disable="true" text="Data Cleaning">
                  <content>
                    <fx:include fx:id="dataCleaningTab" source="DataCleaningTab.fxml" />
                  </content>
                </Tab>
                <Tab fx:id="encodingTabPaneTab" disable="true" text="Encoding">
                  <content>
                    <fx:include fx:id="encodingTab" source="EncodingTab.fxml" />
                  </content>
                </Tab>
                  <Tab disable="true" text="Indexing">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TabPane>
            <fx:include fx:id="dataPane" source="DataPane.fxml" />
         </children>
      </VBox>
   </children>
</AnchorPane>

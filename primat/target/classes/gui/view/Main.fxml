<!-- style-sheet can also be set programatically -->

<BorderPane prefHeight="708.0" prefWidth="997.0" stylesheets="@/gui/style/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dbs.pprl.toolbox.client.gui.controller.MainController">

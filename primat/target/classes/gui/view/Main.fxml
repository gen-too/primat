                     <children>
                        <Group>
                           <children>
                              <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" rotate="90.0">
                                 <image>
                                    <Image url="@../image/pencil.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </Group>
                        <Group>
                           <children>
                              <Label rotate="90.0" text="Selection" />

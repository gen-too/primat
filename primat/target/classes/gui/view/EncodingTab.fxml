<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="dbs.pprl.toolbox.data_owner.gui.controller.EncodingTabController">
   <center>
		<TabPane id="tab-pane-main" fx:id="bloomFilterTabPane" prefHeight="342.0" prefWidth="778.0" tabClosingPolicy="ALL_TABS">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</TabPane>
   </center>
   <bottom>
      <ToolBar id="inner-tab-toolbar" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
         	<Pane HBox.hgrow="ALWAYS" />
      		<Button id="normal-button" fx:id="addBloomFilterDefinitionButton" mnemonicParsing="false" onAction="#addBloomFilterDefinition" prefHeight="26.0" prefWidth="87.0" text="Add BF" />
      		<Button id="confirmButton" fx:id="confirmButton" mnemonicParsing="false" onAction="#confirm" prefHeight="26.0" prefWidth="99.0" styleClass="confirmButton" text="Confirm" />
         </items>
      </ToolBar>
   </bottom>
</BorderPane>
